.item_list {
	display: flex;
  flex-wrap: wrap;
}
.item_box {
  width: calc(50% - 30rpx);
  font-size: 22rpx;
  margin: 10rpx 15rpx;
  padding: 5rpx;
  box-sizing: border-box;
}

.item_box image {
	border-radius: 5px;
}

.item_box .empty {
	line-height: 100px;
	text-align: center;
	background-color: #cccccc;
	height: 100%;
	width: 100%;
	border-radius: 5px;
}

.item_box .middle_box {
	display: flex;
  justify-content: space-between;
  padding-top: 10rpx;
}

.item_box .text {
  color: #cccccc;
}